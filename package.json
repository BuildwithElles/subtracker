{
  "name": "subtracker",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives",
    "lint:strict": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "preview": "vite preview",
    "test:e2e": "playwright test",
    "test:e2e:auth": "playwright test --config=playwright-auth.config.ts",
    "test:e2e:auth:ui": "playwright test --config=playwright-auth.config.ts --ui",
    "test:e2e:auth:headed": "playwright test --config=playwright-auth.config.ts --headed",
    "test:e2e:auth:debug": "playwright test --config=playwright-auth.config.ts --debug",
    "test:e2e:auth:chromium": "playwright test --config=playwright-auth.config.ts --project=chromium",
    "test:e2e:auth:firefox": "playwright test --config=playwright-auth.config.ts --project=firefox",
    "test:e2e:auth:mobile": "playwright test --config=playwright-auth.config.ts --project='Mobile Chrome'",
    "test:e2e:components": "playwright test --config=playwright-auth.config.ts auth-components-e2e.spec.ts",
    "test:e2e:stories": "playwright test --config=playwright-auth.config.ts user-stories-e2e.spec.ts",
    "test:e2e:full": "playwright test --config=playwright-auth.config.ts auth-onboarding-e2e.spec.ts",
    "test:e2e:report": "playwright show-report"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.1",
    "@types/react-router-dom": "^5.3.3",
    "dotenv": "^17.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.9.0",
    "@playwright/test": "^1.54.2",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.4.32",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  }
}
